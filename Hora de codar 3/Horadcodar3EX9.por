programa
{
	funcao inicio()
	{
		// 9 - Escreva um algoritmo para imprimir os 10 primeiros números inteiros maiores que 100.
		
		inteiro contador
		
		escreva("Os 10 primeiros números inteiros maiores que 100 são:\n")
		
		para (contador = 101; contador <= 110; contador = contador + 1) {
			escreva(contador, "\n")
		}
	}
}